{
  "name": "budget-backend",
  "version": "1.0.0",
  "description": "",
  "main": "./src/server.ts",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "ts-node src/server.ts",
    "ext": "ts"
  },
  "scripts": {
    "start": "node build/server.js",
    "pretty": "prettier --find-config-path --write src/**/*.ts{,x}",
    "test": "jest --passWithNoTests ",
    "clean": "ts-node tools/clean.ts",
    "build2": "ts-node tools/build.ts",
    "build": "tsc",
    "build:original": "tsoa spec-and-routes && tsc --project ./",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "routes": "tsoa routes",
    "swagger": "tsoa spec",
    "docs": "typedoc --treatWarningsAsErrors --logLevel Verbose --out docs --entryPointStrategy expand ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.25",
    "@types/puppeteer": "^5.4.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.1.0",
    "faker": "^6.6.6",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "puppeteer": "^13.6.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.3",
    "swagger-node-express": "^2.1.3",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsoa": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-winston": "^4.2.0",
    "fs-extra": "^10.1.0",
    "jsonwebtoken": "^8.5.1",
    "path": "^0.12.7",
    "prettier": "^2.6.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.4",
    "tsconfig-paths": "^3.14.1",
    "typedoc": "^0.22.15",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  }
}
